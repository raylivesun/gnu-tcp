(defmacro call-text (bdc &optional zip
			 :clear info)
  (declare ((bdc &optional zip
		 :clear info)
	    (let* ((done --static
			 :sucessful ok)
		   (if (cond ((shell !)
			      (t (function shell)
				 (:sucessful ok))
			      (t (function text)
				 (:sucessful ok))
			      (t (function tui)
				 (:enable zip))))
		       (then
			(apropos word
				 (if (cond ((shell !)
					    (t (function shell)
					       (:word x/fmt))
					    (t (function shell)
					       (:word bfd))
					    (t (function shell)
					       (:word bdc))))
				     (then
				      (call $args))
				   (else
				    (call $text)))
				 ))
		     (else
		      (call $args)))
		   )))
	   )
  'bdc zip
  :clear info)
				   
